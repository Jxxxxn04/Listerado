<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/guideline4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff">


    <View
        android:id="@+id/change_username_layout_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#fff"
        android:elevation="10dp"
        app:layout_constraintTop_toBottomOf="@+id/myAccount_email"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/change_email_layout_button" />


    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Benutzername ändern"
        android:textColor="#000"
        android:textSize="20sp"
        android:autoSizeMaxTextSize="20sp"
        android:autoSizeMinTextSize="10sp"
        android:autoSizeTextType="uniform"
        android:gravity="center|start"
        android:elevation="11dp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toStartOf="@id/change_username_layout_button"
        app:layout_constraintBottom_toBottomOf="@id/change_username_layout_button"
        app:layout_constraintEnd_toEndOf="@id/change_username_layout_button"
        app:layout_constraintTop_toTopOf="@id/change_username_layout_button" />

    <View
        android:id="@+id/change_email_layout_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#fff"
        android:elevation="10dp"
        android:layout_marginTop="1dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/change_username_layout_button"
        app:layout_constraintBottom_toTopOf="@id/change_password_layout_button" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Email ändern"
        android:textColor="#000"
        android:textSize="20sp"
        android:autoSizeMaxTextSize="20sp"
        android:autoSizeMinTextSize="10sp"
        android:autoSizeTextType="uniform"
        android:gravity="center|start"
        android:elevation="11dp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toStartOf="@id/change_email_layout_button"
        app:layout_constraintBottom_toBottomOf="@id/change_email_layout_button"
        app:layout_constraintEnd_toEndOf="@id/change_email_layout_button"
        app:layout_constraintTop_toTopOf="@id/change_email_layout_button" />


    <View
        android:id="@+id/change_password_layout_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#fff"
        android:elevation="10dp"
        android:layout_marginTop="1dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/change_email_layout_button"
        app:layout_constraintBottom_toTopOf="@id/logout_button" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Passwort ändern"
        android:textColor="#000"
        android:textSize="20sp"
        android:autoSizeMaxTextSize="20sp"
        android:autoSizeMinTextSize="10sp"
        android:autoSizeTextType="uniform"
        android:gravity="center|start"
        android:elevation="11dp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toStartOf="@id/change_password_layout_button"
        app:layout_constraintBottom_toBottomOf="@id/change_password_layout_button"
        app:layout_constraintEnd_toEndOf="@id/change_password_layout_button"
        app:layout_constraintTop_toTopOf="@id/change_password_layout_button" />


    <View
        android:id="@+id/logout_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#fff"
        android:elevation="10dp"
        android:layout_marginTop="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/change_password_layout_button"
        app:layout_constraintBottom_toTopOf="@id/account_delete_layout_button" />


    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Abmelden"
        android:textColor="#000"
        android:textSize="20sp"
        android:autoSizeMaxTextSize="20sp"
        android:autoSizeMinTextSize="10sp"
        android:autoSizeTextType="uniform"
        android:gravity="center|start"
        android:elevation="11dp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toStartOf="@id/logout_button"
        app:layout_constraintBottom_toBottomOf="@id/logout_button"
        app:layout_constraintEnd_toEndOf="@id/logout_button"
        app:layout_constraintTop_toTopOf="@id/logout_button" />


    <View
        android:id="@+id/account_delete_layout_button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#fff"
        android:elevation="10dp"
        android:layout_marginTop="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/logout_button"
        app:layout_constraintBottom_toTopOf="@id/guideline14" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Account Löschen"
        android:textColor="#FF0000"
        android:textSize="20sp"
        android:autoSizeMaxTextSize="20sp"
        android:autoSizeMinTextSize="10sp"
        android:autoSizeTextType="uniform"
        android:gravity="center|start"
        android:elevation="11dp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toStartOf="@id/account_delete_layout_button"
        app:layout_constraintBottom_toBottomOf="@id/account_delete_layout_button"
        app:layout_constraintEnd_toEndOf="@id/account_delete_layout_button"
        app:layout_constraintTop_toTopOf="@id/account_delete_layout_button" />
